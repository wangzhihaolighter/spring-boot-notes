<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.mybatis.mapper.mapper.UserMapper">
  <select id="queryByUsernameLike" resultType="com.example.mybatis.mapper.entity.User">
    select *
    from t_user
    where username like #{username}
  </select>
</mapper>